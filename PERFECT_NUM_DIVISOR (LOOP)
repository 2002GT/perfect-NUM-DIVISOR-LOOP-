#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    int t;
    cin>>t;
    while( t--){
      
      int N , i, sum = 0;
      cin>>N;
      for ( i=1; i<N ; i++){
        if ( N % i == 0)
        sum = sum + i;
      }
      
      if ( sum == N )
      cout<<"true"<<endl;
      
      else 
      cout<<"false"<<endl;
      
    }
    
    return 0;
  }
